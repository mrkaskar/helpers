{
  "entryPoints": ["src/index.ts"],  // Entry point for your TypeScript files
  "out": "docs",                    // Output directory for the generated documentation
  "name": "Helpers of kaskar project",      // The name of your project
  "includeVersion": true,           // Include the version number from package.json in the documentation
  "excludeExternals": true,         // Exclude external libraries from the documentation
  "excludePrivate": true,           // Exclude private members from the documentation
  "excludeProtected": false,        // Include protected members in the documentation
  "excludeInternal": true,          // Exclude internal members from the documentation
  "tsconfig": "tsconfig.json",      // Path to your tsconfig.json file
  "readme": "README.md",            // Path to your README file (or "none" to skip)
  "theme": "default",               // Documentation theme (default, minimal, etc.)
  "exclude": ["**/__tests__/**"],   // Exclude test files or any other files you don't want documented
}
